    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.2.13",
    "react-ga": "^2.5.3",
    "react-loadable": "^5.4.0",
    "react-places-autocomplete": "^7.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-waypoint": "^8.0.3",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
