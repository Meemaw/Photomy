    "gulp-clean": "^0.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-run": "^1.7.1",
    "gulp-template": "^5.0.0",
    "prettier": "^1.14.0",
    "prop-types": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "run-sequence": "^2.2.1",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0"
